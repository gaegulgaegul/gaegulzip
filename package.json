{
  "name": "gaegulzip",
  "private": true,
  "scripts": {
    "demo": "cd apps/mobile/apps/design_system_demo && flutter run",
    "demo:ios": "cd apps/mobile/apps/design_system_demo && flutter run -d 00008030-000538593C7A802E",
    "dev:server": "turbo run dev --filter=gaegulzip-server",
    "dev:admin": "turbo run dev --filter=gaegulzip-admin",
    "dev:wowa": "cd apps/mobile/apps/wowa && flutter run",
    "dev:wowa:ios": "cd apps/mobile/apps/wowa && flutter run -d 00008030-000538593C7A802E",
    "build:server": "turbo run build --filter=gaegulzip-server",
    "build:admin": "turbo run build --filter=gaegulzip-admin",
    "test:server": "turbo run test --filter=gaegulzip-server",
    "mobile:bootstrap": "cd apps/mobile && melos bootstrap",
    "mobile:generate": "cd apps/mobile && melos generate",
    "mobile:clean": "cd apps/mobile && melos clean",
    "ios": "xcrun simctl boot AE1F6125-6BD6-441A-B770-2B6EBA775153 2>/dev/null; open -a Simulator"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "packageManager": "pnpm@10.28.2"
}
